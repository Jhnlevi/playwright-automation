{
    "testCases": [
        {
            "id": "TC_REG_0002",
            "description": "Missing first name for user/customer registration.",
            "type": "invalid_input",
            "data": {
                "user": {
                    "firstName": "",
                    "lastName": "Seed",
                    "address": "29th street",
                    "city": "Los Angeles",
                    "state": "California, USA",
                    "zipCode": "122333",
                    "mobileNumber": "111 222 3333",
                    "ssn": "444455555666666",
                    "userName": "JohnnySeed12",
                    "password": "JohnnyPassword01",
                    "confirmPassword": "JohnnyPassword01"
                }
            },
            "expectedResult": {
                "message": "Registration unsuccessful.",
                "fieldErrors": [
                    {
                        "field": "customer.firstName",
                        "message": "First name is required."
                    }
                ]
            }
        },
