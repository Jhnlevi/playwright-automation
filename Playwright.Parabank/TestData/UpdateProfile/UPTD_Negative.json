{
    "testCases": [
        {
            "id": "TC_UP_0002",
            "description": "Missing first name for update profile process.",
            "type": "invalid_input",
            "data": {
                "profileData": {
                    "firstName": "",
                    "lastName": "Cyrus",
                    "address": "19th avenue",
                    "city": "San Francisco",
                    "state": "Louisiana",
                    "zipCode": "5555",
                    "mobileNumber": "555 777 9999"
                }
            },
            "expectedResult": {
                "message": "Unable to save profile details.",
                "fieldErrors": [
                    {
                        "field": "customer.firstName",
                        "message": "First name is required."
                    }
                ]
            }
        },
