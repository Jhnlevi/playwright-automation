{
    "testCases": [
        {
            "id": "TC_FT_0005",
            "description": "Missing transaction ID for 'Find Transactions' module.",
            "type": "invalid_input",
            "data": {
                "transaction": {
                    "account": "40095",
                    "byId": ""
                }
            },
            "expectedResult": {
                "message": "Process unsuccessful.",
                "fieldErrors": [
                    {
                        "field": "transactionId",
                        "message": "Invalid transaction ID"
                    }
                ]
            }
        },
