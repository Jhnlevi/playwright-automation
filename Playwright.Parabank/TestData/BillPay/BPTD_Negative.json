{
    "testCases": [
        {
            "id": "TC_BP_0002",
            "description": "Missing payee name for bills payment service.",
            "type": "invalid_input",
            "data": {
                "payeeInformation": {
                    "payeeName": "",
                    "payeeAddress": "123 street",
                    "payeeCity": "City of Dreams",
                    "payeeState": "California",
                    "payeeZip": "1111",
                    "payeeMobileNumber": "111 222 3333",
                    "payeeAccountNumber": "14898",
                    "payeeVerifyAccountNumber": "14898",
                    "payeeAmount": "500.00",
                    "payeeFromAccount": "14898"
                }
            },
            "expectedResult": {
                "message": "Bills payment unsuccessful.",
                "fieldErrors": [
                    {
                        "field": "payee.name",
                        "message": "Payee name is required."
                    }
                ]
            }
        },
