{
    "testCases": [
        {
            "id": "TC_LOGIN_0002",
            "description": "Missing user name for user/customer login.",
            "type": "invalid_input",
            "data": {
                "user": {
                    "userName": "",
                    "password": "JohnnyPassword01"
                }
            },
            "expectedResult": {
                "message": "Login unsuccessful.",
                "fieldErrors": [
                    {
                        "field": "error",
                        "message": "Please enter a username and password."
                    }
                ]
            }
        },
        {
